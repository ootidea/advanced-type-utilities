{
  "name": "advanced-type-utilities",
  "version": "0.1.0",
  "description": "A collection library of type utilities for TypeScript.",
  "keywords": [],
  "license": "CC0-1.0",
  "author": "ootidea",
  "sideEffects": false,
  "type": "module",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rimraf dist && tsc --declaration --declarationDir dist --emitDeclarationOnly",
    "fix": "biome check --write",
    "check": "run-p \"check:*\"",
    "check:type": "tsc --noEmit",
    "check:biome": "biome check",
    "prepublishOnly": "pnpm check && pnpm build",
    "postversion": "npm publish ./ && git push --follow-tags"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.0",
    "@tsconfig/strictest": "^2.0.5",
    "npm-run-all2": "^6.2.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.2"
  },
  "packageManager": "pnpm@9.10.0"
}
